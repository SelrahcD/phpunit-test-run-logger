{
  "name": "selrahcd/phpunit-test-run-logger",
  "description": "A test run logger for PHPUnit. Records the results of test runs for improving TDD workflow.",
  "type": "library",
  "require": {
    "php": "~8.0.0",
    "phpunit/phpunit": "^9.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.8",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-phpunit": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Selrahcd\\PhpunitTestRunLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Selrahcd\\PhpunitTestRunLogger\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "SelrahcD",
      "email": "c.desneuf@gmail.com"
    }
  ]
}
